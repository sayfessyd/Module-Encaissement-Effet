<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="com.pfe.model.TypePiece" table="TYPE_PIECE"
		rowid="rowid">
		<id name="codTpceTpce" type="java.lang.Long">
			<column name="COD_TPCE_TPCE" precision="2" scale="0" />
			<generator class="assigned" />
		</id>
		<property name="libTpceTpce" type="java.lang.String">
			<column name="LIB_TPCE_TPCE" length="30" not-null="true" />
		</property>
		<property name="libSiglTpce" type="java.lang.String">
			<column name="LIB_SIGL_TPCE" length="10" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
