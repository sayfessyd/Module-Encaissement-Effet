<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.pfe.model.MotifRejet" rowid="rowid" table="MOTIF_REJET">
		<id name="codeRej" type="java.lang.Long">
			<column name="CODE_REJ" precision="3" scale="0" />
			<generator class="assigned" />
		</id>
		<property name="motifRej" type="java.lang.String">
			<column name="MOTIF_REJ" length="60" />
		</property>
	</class>
</hibernate-mapping>
